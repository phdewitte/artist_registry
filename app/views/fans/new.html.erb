<%= form_for @fan do |f| %>
  First name: <%= f.text_field :first_name %>
  Last name: <%= f.text_field :last_name %>
  Email: <%= f.text_field :email %>
  Password <%= f.password_field :password %>
  Location: <%= f.text_field :location %>
  Photo URL: <%= f.text_field :photo_url %>
  <%= f.submit %>
<% end %>